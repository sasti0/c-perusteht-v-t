usning System;
class calculator
{
static void Main(string[] args)
{
    Console.writeLine("NeliÃ¶n sivu?");
    double sivu = covert.ToDouble(console.readline());
    if (sivu >= 0)
        {
            double pintaAla = Math.Pow(sivu, 2);
            console.writeLine($"Pinta-ala on {pintaAla:F2}");
        }
        else
        {
            console.write("error");
        }
}
}